{
  "manifest_version": 3,
  "name": "Find my meeting room",
  "version": "1.0",
  "description": "Find the meeting room by providing the meeting room ID",
  "action": {
    "default_icon": "images/building.png"
  },
  "permissions": ["tabs"],
  "background": {
    "service_worker": "scripts/background.js",
    "type": "module"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+Z",
        "mac": "Command+Shift+Z"
      }
    }
  }
}